{
    "name": "Dotnet Development",
    "dockerComposeFile": ["../docker-compose.yaml"],
    "workspaceFolder": "/app",
    "service": "dotnet-app",
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-azuretools.vscode-docker"
            ],
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            }
        }
    },
    "forwardPorts": [
        5000
    ]
}
  